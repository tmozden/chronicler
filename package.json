    "config": "^3.3.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/config": "0.0.39",
    "@types/cors": "^2.8.12",
